    },
    "uuid": {
      "type": "string",
      "format": "uuid"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "sender": {
      "type": "string"
    },
    "receiver": {
      "type": "string"
    },
    "message_id": {
      "type": "string"
    },
    "body": {
      "type": "string"
    }
  },
  "required": ["type", "timestamp", "sender", "receiver", "message_id", "body"]
}
