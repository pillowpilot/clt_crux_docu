      },
      "conversation_id": {
        "type": "string"
      },
      "body": {
        "type": "string"
      }
    },
    "required": [
      "type",
      "uuid",
      "timestamp",
      "flow_execution_id",
      "flow_type",
      "conversation_id",
      "body"
    ]
  }
  