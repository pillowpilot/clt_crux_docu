{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Send WhatsApp Message",
  "type": "object",
  "properties": {
    "uuid": {
      "type": "string",
      "format": "uuid"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "sender": {
      "type": "string"
    },
    "receiver": {
      "type": "string"
    },
    "body": {
      "type": "string"
    }
  },
  "required": ["uuid", "timestamp", "sender", "receiver", "body"]
}
